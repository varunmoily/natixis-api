package com.example.demodb.Model1;

public class Student1 {
	private int id;
	private String name;
	private int fees;
	public Student1() {
		super();
		// TODO Auto-generated constructor stub
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getFees() {
		return fees;
	}
	public void setFees(int fees) {
		this.fees = fees;
	}
	@Override
	public String toString() {
		return "Student1 [id=" + id + ", name=" + name + ", fees=" + fees + "]";
	}
	

}
